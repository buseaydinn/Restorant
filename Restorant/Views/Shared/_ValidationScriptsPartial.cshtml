<script src="~/lib/jquery-validation/dist/jquery.validate.min.js"></script>
<script src="~/lib/jquery-validation-unobtrusive/jquery.validate.unobtrusive.min.js"></script>
<script>
    function temizle() {
        var maaşInput = document.getElementById("validationCustom01");
        var maaşDeğeri = maaşInput.value;

        // Başındaki sıfırları sil
        maaşDeğeri = maaşDeğeri.replace(/^0+/, '');

        // Input alanına temizlenmiş değeri yaz
        maaşInput.value = maaşDeğeri;
    }
</script>