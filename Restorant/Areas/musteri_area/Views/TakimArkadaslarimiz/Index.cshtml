    <div id="team-page" class="page-hero-section division">
        <div class="container">
            <div class="row">
                <div class="col-lg-10 offset-lg-1">
                    <div class="hero-txt text-center white-color">
                        <div id="breadcrumb">
                            <div class="row">
                                <div class="col">
                                    <div class="breadcrumb-nav">
                                        <nav aria-label="breadcrumb">
                                            <ol class="breadcrumb">
                                            
                                            </ol>
                                        </nav>
                                    </div>
                                </div>
                            </div>
                        </div>

                        <h2 class="h2-xl">Takım Arkadaşlarımız</h2>

                    </div>
                </div>
            </div>	 
        </div>	  
    </div>	
   
    <section id="team-1" class="team-1 wide-60 reviews-section division">

        <div class="container">

            <div class="team-1-wrapper text-center">

                <div class="row">

                    @foreach (var personel in ViewBag.Personeller)
                    {
                        <div class="col-sm-6 col-lg-3">
                            <div class="team-member">

                         
                                <div class="team-member-photo">

                                    <img class="img-fluid" src="~/img/@personel.Fotograf" alt="team-member-foto">
                                </div>

                                <div class="tm-meta">
                                    <h5 class="h5-sm">@personel.Ad @personel.Soyad</h5>
                                    <h3 class="h5-sm">@personel.Rol.Ad</h3>
                                  
                                </div>

                            </div>
                        </div>
                    }
                 </div>
            </div>	

        </div>	    
    </section>	
   
    <div class="bg-color-01 page-pagination division">
        <div class="container">
            <div class="row">
                <div class="col-md-12">

                    <nav aria-label="Page navigation">
                        <ul class="pagination justify-content-center">
                            <li class="page-item disabled"><a class="page-link" href="#" tabindex="-1"><i class="fas fa-angle-left"></i></a></li>
                            <li class="page-item active"><a class="page-link" href="#">1 <span class="sr-only">(current)</span></a></li>
                            <li class="page-item"><a class="page-link" href="#">2</a></li>
                            <li class="page-item"><a class="page-link" href="#">3</a></li>
                            <li class="page-item"><a class="page-link" href="#">4</a></li>
                            <li class="page-item"><a class="page-link" href="#"><i class="fas fa-angle-right"></i></a></li>
                        </ul>
                    </nav>

                </div>
            </div>  
        </div> 
    </div>

    <section id="banner-4" class="bg-fixed wide-100 banner-section division">

        <div class="container">

            <div class="row">


                <div class="col-lg-10 col-xl-8 offset-lg-1 offset-xl-2">

                    <div class="banner-4-txt text-center">

                        <h4 class="h4-xl">

                            30 DAKİKADA
                        </h4>

                        <h2>
                            TESLİMAT GARANTİSİ!
                        </h2>

                        <p class="p-lg">

                            30 dakikada teslimat garantisiyle, siparişinizin en kısa sürede kapınıza ulaşmasını sağlıyoruz. Hızlı ve güvenilir hizmet anlayışımızla müşteri memnuniyetini en üst düzeyde tutuyoruz!
                        </p>

                    </div>

                </div>

            </div>
        </div>

    </section>

</div>

